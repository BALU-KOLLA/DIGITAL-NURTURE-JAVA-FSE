import java.util.*;
public class Main
{
	public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        String str = s.nextLine();
        int sum = 0;
        char ch[] = str.toCharArray();
        for(int i=0;i<str.length()-1;i++){
            int num1 = ch[i] - '0';      
            int num2 = ch[i + 1] - '0';  
            sum += (num1 - num2);
        }
        System.out.println("cumulative sum "+Math.abs(sum));
    }
}
