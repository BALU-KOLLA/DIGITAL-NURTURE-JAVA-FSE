import java.util.*;
public class Main
{
	public static void main(String args[]){
	    int sum=0;
	    Scanner s = new Scanner(System.in);
	    int n = s.nextInt();
	    double a1 = 0.0f;
	    int[] a = new int[5];
	    for(int i=0;i<n;i++){
	        a[i] = s.nextInt();
	        sum += a[i];
	    }
	    a1 = (double)sum/n;
	    System.out.println(sum+" "+a1);
	}
		
} 
